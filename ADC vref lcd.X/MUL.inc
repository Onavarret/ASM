;PARAMETER (N1, MULTIPLIER)
;RETURN MULTI ; RESULT OF N1 ADDED MULTIPLIER TIMES

    


    
    
MULTIPLICATION:
    
		CBLOCK	0X020
		N1    ;FIRST CONSTANT OF THE MULTIPLICATION
		MULTIPLIER  ;TIMES THAT CONSTANT IS GOING TO BE ADDED
		MULTI ;VARIABLE WHERE RESULT IS GOING TO BE PLACED
		ENDC
	
           	;;;;;;;;;;;;;;;;;
		
		BANKSEL MULTI
		CLRF    MULTI     ;CLEAR REGISTER
		CLRW              ;CLEAR WORKIGN REGISTER
BUCLE_MUL:
		BANKSEL N1
		ADDWF   N1,0
		BANKSEL MULTI
		MOVWF   MULTI
		;BANKSEL MULTIPLIER
		DECFSZ  MULTIPLIER
		
		GOTO    BUCLE_MUL
		
		RETURN
		
		
;END